<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>Timesheet</name>
      <parent>TKDocument</parent>
      <description>Timesheet Document</description>
      <label>Timesheet Document</label>
      <postProcessorName>edu.iu.uis.hr.tk.workflow.PostProcessor</postProcessorName>
      <superUserWorkgroupName>TimeSystemSuperusers</superUserWorkgroupName>
      <blanketApproveWorkgroupName>TimeBlanketApprovers</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>TK_INTERFACE_MANAGER</defaultExceptionWorkgroupName>
      <docHandler>${TK.DocHandlerPrefix}/TimesheetDocument.do?method=open</docHandler>
      <active>true</active>
      <attributes>
        <attribute>
          <name>TimesheetDepartmentSearchAttribute</name>
        </attribute>
        <attribute>
          <name>TimesheetWorkareaSearchAttribute</name>
        </attribute>
        <attribute>
          <name>TimesheetActionListAttribute</name>
        </attribute>
        <attribute>
          <name>TimesheetNoteAttribute</name>
        </attribute>
      </attributes>
      <routingVersion>2</routingVersion>
      <routePaths>
        <routePath>
          <start name="Employee" nextNode="Supervisor" />
          <requests name="Supervisor" nextNode="PayrollProcessor" />
          <requests name="PayrollProcessor" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Employee">
          <activationType>P</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Supervisor">
          <activationType>P</activationType>
          <ruleTemplate>SupervisorRuleTemplate</ruleTemplate>
          <mandatoryRoute>true</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="PayrollProcessor">
          <activationType>P</activationType>
          <ruleTemplate>PayrollProcessorRuleTemplate</ruleTemplate>
          <mandatoryRoute>true</mandatoryRoute>
          <finalApproval>true</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
