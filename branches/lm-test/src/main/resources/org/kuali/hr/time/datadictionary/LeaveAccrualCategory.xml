<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="LeaveAccrualCategory" parent="LeaveAccrualCategory-parentBean" />

	<bean id="LeaveAccrualCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.lm.leaveaccrualcategory.LeaveAccrualCategory" />
		<property name="titleAttribute" value="id" />
		<property name="objectLabel" value="LeaveAccrualCategory" />
		<property name="inquiryDefinition">
			<ref bean="LeaveAccrualCategory-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="LeaveAccrualCategory-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="LeaveAccrualCategory-lmLeaveAccrualCategoryId" />
				<ref bean="LeaveAccrualCategory-accrualCategory" />
				<ref bean="LeaveAccrualCategory-description" />
				<ref bean="LeaveAccrualCategory-accrualEarnInterval" />
				<ref bean="LeaveAccrualCategory-leavePlan" />
				<ref bean="LeaveAccrualCategory-effectiveDate" />
				<ref bean="LeaveAccrualCategory-proration" />
				<ref bean="LeaveAccrualCategory-showOnGrid" />
				<ref bean="LeaveAccrualCategory-timestamp" />
			</list>
		</property>
		<property name="relationships">
  			<list>
	           <bean parent="RelationshipDefinition">
	             <property name="objectAttributeName" value="leavePlanObj"/>
	             <property name="primitiveAttributes">
	               <list>
	                 <bean parent="PrimitiveAttributeDefinition" p:sourceName="leavePlan" p:targetName="leavePlan"/>
	               </list>
	             </property>
	           </bean>
         	</list>
         </property>	 
	</bean>

	<!-- Attribute Definitions -->

	<bean id="LeaveAccrualCategory-lmLeaveAccrualCategoryId" parent="LeaveAccrualCategory-lmLeaveAccrualCategoryId-parentBean" />
	<bean id="LeaveAccrualCategory-lmLeaveAccrualCategoryId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Leave Plan Id" />
		<property name="maxLength" value="7" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="7" />
		</property>
		<property name="name" value="lmLeaveAccrualCategoryId" />
		<property name="label" value="Leave Accrual Category Id" />
	</bean>


	<bean id="LeaveAccrualCategory-accrualCategory" parent="LeaveAccrualCategory-accrualCategory-parentBean" />
	<bean id="LeaveAccrualCategory-accrualCategory-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Accrual Category" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="15" />
		</property>
		<property name="name" value="accrualCategory" />
		<property name="label" value="Accrual Category" />
	</bean>

	<bean id="LeaveAccrualCategory-description" parent="LeaveAccrualCategory-description-parentBean" />
	<bean id="LeaveAccrualCategory-description-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Description" />
		<property name="maxLength" value="30" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="true" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="description" />
		<property name="label" value="Description" />
	</bean>
	
	<bean id="LeaveAccrualCategory-leavePlan" parent="LeaveAccrualCategory-leavePlan-parentBean" />
	<bean id="LeaveAccrualCategory-leavePlan-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="LeavePlan" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="true" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="15" />
		</property>
		<property name="name" value="leavePlan" />
		<property name="label" value="Leave Plan" />
	</bean>

	<bean id="LeaveAccrualCategory-effectiveDate" parent="LeaveAccrualCategory-effectiveDate-parentBean" />
	<bean id="LeaveAccrualCategory-effectiveDate-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Effective Date" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="DateValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
		</property>
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
		<property name="name" value="effectiveDate" />
		<property name="label" value="Effective Date" />
	</bean>
	
	<bean id="LeaveAccrualCategory-accrualEarnInterval" parent="LeaveAccrualCategory-accrualEarnInterval-parentBean" />
	<bean id="LeaveAccrualCategory-accrualEarnInterval-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Accrual Earn Interval" />
		<property name="maxLength" value="1" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.time.util.lmAccrualEarnIntervalValues" />
		</property>
		<property name="name" value="accrualEarnInterval" />
		<property name="label" value="Accrual Earn Interval" />
	</bean>

	

	<bean id="LeaveAccrualCategory-proration" parent="LeaveAccrualCategory-proration-parentBean" />
	<bean id="LeaveAccrualCategory-proration-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Proration" />
		<property name="maxLength" value="1" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.time.util.lmProrationValues"/>
		</property>
		<property name="name" value="proration" />
		<property name="label" value="Proration" />
	</bean>

<bean id="LeaveAccrualCategory-showOnGrid" parent="LeaveAccrualCategory-showOnGrid-parentBean" />
	<bean id="LeaveAccrualCategory-showOnGrid-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Show On Grid" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="showOnGrid" />
		<property name="label" value="Show On Grid" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>

	<bean id="LeaveAccrualCategory-timestamp" parent="LeaveAccrualCategory-timestamp-parentBean" />
	<bean id="LeaveAccrualCategory-timestamp-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Timestamp" />
		<property name="maxLength" value="15" />

		<property name="control">
			<bean parent="LookupReadonlyControlDefinition" p:size="15" />
		</property>
		<property name="formatterClass"
			value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter" />
		<property name="name" value="timeStamp" />
		<property name="label" value="Time Stamp" />
	</bean>
	
	<!--  Inquiry Definitions  -->
  
  	<bean id="LeaveAccrualCategory-inquiryDefinition" parent="LeaveAccrualCategory-inquiryDefinition-parentBean" />

	<bean id="LeaveAccrualCategory-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
							<bean parent="FieldDefinition" p:attributeName="leavePlan" />
							<bean parent="FieldDefinition" p:attributeName="description" />
						</list>
					</property>
					<property name="title" value="LeaveAccrualCategory" />
				</bean>
			</list>
		</property>
		<property name="title" value="LeaveAccrualCategory Inquiry" />
	</bean>

	<!-- Look up Definitions  -->

	<bean id="LeaveAccrualCategory-lookupDefinition" parent="LeaveAccrualCategory-lookupDefinition-parentBean" />
	<bean id="LeaveAccrualCategory-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupableID" value="leaveAccrualCategoryLookupable"></property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
				<bean parent="FieldDefinition" p:attributeName="leavePlan" />
				<bean parent="FieldDefinition" p:attributeName="description" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
				<bean parent="FieldDefinition" p:attributeName="description" />
				<bean parent="FieldDefinition" p:attributeName="leavePlan" />
				<bean parent="FieldDefinition" p:attributeName="accrualEarnInterval" />
				<bean parent="FieldDefinition" p:attributeName="proration" />
				<bean parent="FieldDefinition" p:attributeName="showOnGrid" />
			</list>
		</property>
		<property name="title" value="Leave Accrual Category Lookup" />
	</bean>

</beans>

