<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="KhrEmployeeDocument" parent="KhrEmployeeDocument-parentBean"/>
    <bean id="KhrEmployeeDocument-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="dataObjectClass" value="org.kuali.kpme.core.tkmdocument.KhrEmployeeDocument"/>
        <property name="objectLabel" value="KhrEmployeeDocument"/>
        <property name="attributes">
            <list>
                <ref bean="KhrEmployeeDocument-principalId" />
                <ref bean="KhrEmployeeDocument-name"/>
                <ref bean="KhrEmployeeDocument-startDate"/>
                <ref bean="KhrEmployeeDocument-endDate"/>
                <ref bean="KhrEmployeeDocument-process"/>
            </list>
        </property>
        <property name="collections">
            <list>
                <bean parent="CollectionDefinition">
                    <property name="dataObjectClass" value="org.kuali.kpme.core.tkmdocument.tkmjob.KhrEmployeeJob"/>
                    <property name="name" value="jobList"/>
                    <property name="shortLabel" value="Jobs"/>
                    <property name="label" value="Jobs"/>
                </bean>
            </list>
        </property>
        <property name="primaryKeys">
            <list>
                <value>principalId</value>
            </list>
        </property>
    </bean>

    <bean id="KhrEmployeeDocument-principalId" parent="KhrEmployeeDocument-principalId-parentBean"/>
    <bean id="KhrEmployeeDocument-principalId-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="10" />
        </property>
        <property name="name" value="principalId" />
        <property name="label" value="Principal Id" />
    </bean>

    <bean id="KhrEmployeeDocument-name" parent="KhrEmployeeDocument-name-parentBean"/>
    <bean id="KhrEmployeeDocument-name-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="10" />
        </property>
        <property name="name" value="name" />
        <property name="label" value="Name" />
    </bean>

    <bean id="KhrEmployeeDocument-startDate" parent="KhrEmployeeDocument-startDate-parentBean"/>
    <bean id="KhrEmployeeDocument-startDate-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="controlField">
            <bean parent="Uif-DateControl"/>
        </property>
        <property name="name" value="startDate" />
        <property name="label" value="Start Date" />
    </bean>

    <bean id="KhrEmployeeDocument-endDate" parent="KhrEmployeeDocument-endDate-parentBean"/>
    <bean id="KhrEmployeeDocument-endDate-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="controlField">
            <bean parent="Uif-DateControl"/>
        </property>
        <property name="name" value="endDate" />
        <property name="label" value="End Date" />
    </bean>

    <bean id="KhrEmployeeDocument-process" parent="KhrEmployeeDocument-process-parentBean"/>
    <bean id="KhrEmployeeDocument-process-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="controlField">
            <bean parent="Uif-DropdownControl" />
        </property>
        <property name="optionsFinder">
            <bean class="org.kuali.kpme.core.tkmdocument.web.KhrEmployeeDocumentProcessKeyValueFinder"/>
        </property>
        <property name="name" value="process" />
        <property name="label" value="Process" />
    </bean>

    <!--***************************************************************************
                            Views
    ***************************************************************************-->

    <bean id="KhrEmployeeDocument-InquiryView" parent="Uif-InquiryView" >
        <property name="headerText" value="TKM Document"/>
        <property name="dataObjectClassName" value="org.kuali.kpme.core.tkmdocument.KhrEmployeeDocument"/>
        <property name="singlePageView" value="true"/>
        <property name="items">
            <list>
                <bean parent="Uif-Disclosure-GridSection" >
                    <property name="layoutManager.numberOfColumns" value="2" />
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="principalId"/>
                            <bean parent="Uif-DataField" p:propertyName="name" />
                            <bean parent="Uif-DataField" p:propertyName="startDate" />
                            <bean parent="Uif-DataField" p:propertyName="endDate" />
                        </list>
                    </property>
                    <property name="title" value="Person Document" />
                </bean>
                <bean parent="KhrEmployeeDocument-jobList-inquiry"/>
            </list>
        </property>
    </bean>

    <!-- Job List, used by inquiry view -->
    <bean id="KhrEmployeeDocument-jobList-inquiry" abstract="true" parent="Uif-Disclosure-TableCollectionSection">
        <property name="collectionObjectClass" value="org.kuali.kpme.core.tkmdocument.tkmjob.KhrEmployeeJob" />
        <property name="propertyName" value="jobList" />
        <property name="headerText" value="Job" />
        <property name="layoutManager.renderSequenceField" value="false"/>
        <property name="layoutManager.separateAddLine" value="true"/>
        <property name="layoutManager.richTable.render" value="false"/>
        <property name="items">
            <list>
                <bean parent="Uif-DataField" p:propertyName="effectiveDate" />
                <bean parent="Uif-DataField" p:propertyName="endDate" />
                <bean parent="Uif-DataField" p:propertyName="dept" />
                <bean parent="Uif-DataField" p:propertyName="compRate" />
                <bean parent="Uif-DataField" p:propertyName="standardHours"/>
                <bean parent="Uif-DataField" p:propertyName="payGrade" />
            </list>
        </property>
        <property name="layoutManager.rowDetailsGroup">
            <bean parent="Uif-TableSubCollection-WithinSection" id = "KhrEmployee-AddAssignment">
                <property name="collectionObjectClass" value="org.kuali.kpme.core.assignment.AssignmentBo"/>
                <property name="propertyName" value="assignments" />
                <property name="headerText" value="Assignment" />
                <property name="layoutManager.renderSequenceField" value="false"/>
                <property name="layoutManager.separateAddLine" value="true"/>
                <property name="layoutManager.richTable.render" value="false"/>
                <property name="items">
                    <list>
                        <bean parent="Uif-DataField" p:propertyName="effectiveDate" />
                        <bean parent="Uif-DataField" p:propertyName="workarea" />
                        <bean parent="Uif-DataField" p:propertyName="task" />
                        <bean parent="Uif-DataField" p:propertyName="active" />
                    </list>
                </property>
                <property name="layoutManager.rowDetailsGroup">
                    <bean parent="Uif-TableSubCollection-WithinSection"  id = "KhrEmployee-AddAssignmentAccount">
                        <property name="collectionObjectClass" value="org.kuali.kpme.core.assignment.account.AssignmentAccountBo"/>
                        <property name="propertyName" value="assignmentAccounts" />
                        <property name="headerText" value="Assignment Accounts" />
                        <property name="layoutManager.renderSequenceField" value="false"/>
                        <property name="layoutManager.separateAddLine" value="true"/>
                        <property name="layoutManager.richTable.render" value="false"/>
                        <property name="items">
                            <list>
                                <bean parent="Uif-DataField" p:propertyName="accountNbr" />
                                <bean parent="Uif-DataField" p:propertyName="subAcctNbr" />
                                <bean parent="Uif-DataField" p:propertyName="earnCode" />
                                <bean parent="Uif-DataField" p:propertyName="percent" />
                                <bean parent="Uif-DataField" p:propertyName="active" />
                            </list>
                        </property>
                    </bean>
                </property>
            </bean>
        </property>
    </bean>

    <!-- Look up Definitions -->
    <bean id="KhrEmployeeDocument-LookupView" parent="Uif-LookupView">
        <property name="headerText" value="TKM Document Lookup" />
        <property name="dataObjectClassName" value="org.kuali.kpme.core.tkmdocument.KhrEmployeeDocument" />
        <property name="viewHelperServiceClass" value="org.kuali.kpme.core.tkmdocument.web.KhrEmployeeDocumentLookupableImpl"/>

