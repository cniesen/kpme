    <bean id="KhrEmployeeDocumentMaintenanceDocument" parent="KhrEmployeeDocumentMaintenanceDocument-parentBean" />
    <bean id="KhrEmployeeDocumentMaintenanceDocument-parentBean" abstract="true"
          parent="uifMaintenanceDocumentEntry"
          p:dataObjectClass="org.kuali.kpme.core.tkmdocument.KhrEmployeeDocument"
          p:maintainableClass="org.kuali.kpme.core.tkmdocument.web.KhrEmployeeMaintainableImpl"
          p:businessRulesClass="org.kuali.kpme.core.tkmdocument.validation.KhrEmployeeDocumentValidation"
          p:documentAuthorizerClass="org.kuali.kpme.core.tkmdocument.authorization.KhrEmployeeDocumentMaintenanceDocumentAuthorizer" >

        <property name="documentTypeName" value="KhrEmployeeDocumentType" />

        <property name="lockingKeys">
            <list>
                <value>principalId</value>
            </list>
        </property>
    </bean>




    <bean id="KhrEmployeeDocument-MaintenanceView" parent="KhrEmployeeDocument-MaintenanceView-parentBean" />
