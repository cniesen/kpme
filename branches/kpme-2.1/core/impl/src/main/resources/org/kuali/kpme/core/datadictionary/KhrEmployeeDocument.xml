    <bean id="KhrEmployeeDocument-jobSection-maintenance" abstract="true" parent="Uif-TableCollectionSection">
        <property name="collectionObjectClass" value="org.kuali.kpme.core.tkmdocument.tkmjob.KhrEmployeeJob" />
        <property name="propertyName" value="jobList" />
        <property name="headerText" value="Jobs" />

        <property name="renderLineActions" value="true" />
        <property name="renderAddLine" value="@{#dp.process eq 'New Job/Payrate'}" />


        <property name="addLinePlacement" value="TOP"/>

        <property name="addLineActions">
            <list>
                <bean parent="Uif-AddLineAction" id = "KhrEmployee-AddBlankJob">
                    <property name="actionLabel" value="Add Job"></property>
                    <property name="render" value="true" />
                    <property name="refreshWhenChangedPropertyNames" value="#dp.process" />
                </bean>
            </list>
        </property>
        <property name="lineActions">
            <list>
                <bean parent="Uif-SecondaryActionButton-Small" p:methodToCall="deleteLine"
                      p:actionLabel="delete" p:render="@{ (  (#line.id eq null) and (#dp.process eq 'New Job/Payrate')  ) }" />
            </list>
        </property>

        <property name="layoutManager.expandDetailsActionPrototype">
            <bean parent="Uif-ExpandDetailsImageAction"/>
        </property>

        <property name="layoutManager.separateAddLine" value="true"/>
        <property name="layoutManager.richTable.render" value="true"/>
        <property name="layoutManager.ajaxDetailsRetrieval" value="true"/>
        <property name="layoutManager.renderSequenceField" value="false"/>
        <property name="layoutManager.generateAutoSequence" value="true" />

        <property name="layoutManager.richTable.templateOptions">
            <map>
                <entry key="bPaginate" value="false" />
                <entry key="bFilter" value="false"/>
                <entry key="bInfo" value="false"/>
            </map>
        </property>

        <property name="addLineItems">
            <list>
                <bean parent="Uif-InputField" p:propertyName="effectiveDate" p:required="true"/>
                <bean parent="Uif-InputField" p:propertyName="endDate" p:required="false"/>
                <bean parent="Uif-InputField" p:propertyName="dept" p:required="true">
                    <property name="quickfinder">
                        <bean parent="Uif-QuickFinderByScript"
                              p:dataObjectClassName="org.kuali.kpme.core.department.DepartmentBo"
                              p:fieldConversions="dept:dept"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:propertyName="compRate" p:required="true"/>
                <bean parent="Uif-InputField" p:propertyName="standardHours" p:required="true" />
                <bean parent="Uif-InputField" p:propertyName="payGrade" p:required="true" p:readOnly="false" >
                    <property name="quickfinder">
                        <bean parent="Uif-QuickFinderByScript"
                              p:dataObjectClassName="org.kuali.kpme.core.paygrade.PayGradeBo"
                              p:fieldConversions="payGrade:payGrade"/>
                    </property>
                </bean>
            </list>
        </property>


        <property name="showInactiveLines" value = "true"/>
        <property name="renderInactiveToggleButton" value="false"/>

        <property name="items">
            <list>
                <!-- "@{!(#line.objectId eq null)} -->
                <bean parent="Uif-InputField" p:propertyName="jobNumber" p:readOnly="true" p:readOnlyDisplayReplacementPropertyName="jobNumberDisplay"/>

                <!-- @{ (#dp.process eq '' or #dp.process eq null or #dp.process eq 'Update Job Details' or #dp.process eq 'New Job/Payrate' or #dp.process eq 'Change Job Dates' or #dp.process eq 'Update Job Details' or #dp.process eq 'End Job')} -->
                <bean parent="Uif-InputField" p:propertyName="effectiveDate" p:required="true"
                	p:readOnly="@{ ! (#dp.process eq 'Change Job Dates')}"/>

                <bean parent="Uif-InputField" p:propertyName="endDate"
                	p:readOnly="@{ ! (#dp.process eq 'Change Job Dates')}" />

                <bean parent="Uif-InputField" p:propertyName="dept" p:required="true"
                	p:readOnly="true" >
                    <property name="quickfinder">
                        <bean parent="Uif-QuickFinderByScript"
                             p:dataObjectClassName="org.kuali.kpme.core.department.DepartmentBo"
                             p:fieldConversions="dept:dept"/>
                    </property>
                </bean>

                <bean parent="Uif-InputField" p:propertyName="compRate" p:required="true"
                	p:readOnly="@{!  (#dp.process eq 'Change Job Dates' or #dp.process eq 'Update Job Details' or #dp.process eq 'End Job') }" />

                <bean parent="Uif-InputField" p:propertyName="standardHours"
                	p:readOnly="true" />

                <bean parent="Uif-InputField" p:propertyName="payGrade"
                	p:readOnly="true" >
                    <property name="quickfinder">
                        <bean parent="Uif-QuickFinderByScript"
                              p:dataObjectClassName="org.kuali.kpme.core.paygrade.PayGradeBo"
                              p:fieldConversions="payGrade:payGrade"/>
                    </property>
                </bean>
            </list>
        </property>

