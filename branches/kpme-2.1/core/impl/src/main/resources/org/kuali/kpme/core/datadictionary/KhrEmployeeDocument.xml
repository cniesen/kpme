        <property name="layoutManager.rowDetailsGroup">
            <bean parent="Uif-TableSubCollection-WithinSection" id = "KhrEmployee-Assignment-Collection">
                <property name="header">
                    <bean parent="Uif-SubCollectionHeader">
                        <property name="lowerGroup">
                            <bean parent="Uif-InactiveItemsActionsGroup">
                                <property name="hidden" value = "@{ (!(#dp.process eq 'New Job/Payrate'))}"/>
                                <property name="items">
                                    <list merge="true">
                                        <bean parent="Uif-SecondaryActionButton-Small"
                                              p:actionLabel="Toggle Add Assignment Fields" p:refreshId="@{#collectionGroup.id}"
                                              p:preSubmitCall="if (jQuery('#@{#collectionGroup.layoutManager.addLineGroup.id}').is(':visible'))
                                               {
                                                   jQuery('#@{#collectionGroup.layoutManager.addLineGroup.id}').hide();
                                               }
                                               else
                                               {
                                                   jQuery('#@{#collectionGroup.layoutManager.addLineGroup.id}').show();
                                               }return false;">
                                            <property name="hidden" value = "@{ (!(#dp.process eq 'New Job/Payrate'))}"/>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                        </property>
                    </bean>
                </property>

                <property name="collectionObjectClass" value="org.kuali.kpme.core.assignment.AssignmentBo"/>
                <property name="propertyName" value="assignments" />
                <property name="headerText" value="Assignment" />

                <property name="addLineActions">
                    <list>
                    <bean parent="Uif-AddLineAction" id = "KhrEmployee-AddBlankAssignment">
                        <property name="actionLabel" value="Add Assignment"></property>
