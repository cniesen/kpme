<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
    	<form-bean name="TemporaryIndexForm" type="edu.iu.uis.hr.client.TemporaryIndexForm"/>
	</form-beans>

	<global-forwards type="org.apache.struts.action.ActionForward">
	</global-forwards>

	<action-mappings>
		<action path="/*Lookup" name="{1}LookupForm" type="edu.iu.uis.hr.client.LookupAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/lookup/{1}Lookup.jsp"/>
		</action>
		<action path="/*Inquiry" name="{1}InquiryForm" type="edu.iu.uis.hr.client.LookupAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/inquiry/{1}Inquiry.jsp"/>
		</action>
		<action path="/*MaintenanceLookup" name="{1}MaintenanceLookupForm" type="edu.iu.uis.hr.client.MaintenanceLookupAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/lookup/{1}MaintenanceLookup.jsp"/>
		</action>
		<action path="/*Maintenance" name="{1}MaintenanceForm" type="edu.iu.uis.hr.client.MaintenanceAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/maintenance/{1}Maintenance.jsp"/>
		</action>
		<action path="/*Document" name="{1}DocumentForm" type="edu.iu.uis.hr.client.DocumentAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/document/{1}Document.jsp"/>
		</action>				
		<action path="/TemporaryIndex" name="TemporaryIndexForm" type="edu.iu.uis.hr.client.TemporaryIndexAction" scope="request" validate="false" parameter="method">
			<forward name="default" path="/WEB-INF/jsp/TemporaryIndex.jsp"/>
		</action>
					
	</action-mappings>
  
    <controller processorClass="edu.iu.uis.hr.client.StrutsRequestProcessor"/>
    
   	<plug-in className="edu.iu.uis.hr.client.ApplicationStrutsPlugin"/>
</struts-config>