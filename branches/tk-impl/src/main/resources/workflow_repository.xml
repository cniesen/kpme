
<descriptor-repository version="1.0" isolation-level="read-uncommitted">
  <jdbc-connection-descriptor 
    jcd-alias="EDEN"
    default-connection="false"
    platform="Oracle9i"
    jdbc-level="3.0"
    eager-release="false"
    batch-mode="false"
    useAutoCommit="0"
    ignoreAutoCommitExceptions="false">
    <sequence-manager className="org.apache.ojb.broker.util.sequence.SequenceManagerNextValImpl" />
    <object-cache class="org.apache.ojb.broker.cache.ObjectCachePerBrokerImpl"/>
  </jdbc-connection-descriptor>

<class-descriptor class="edu.iu.uis.eden.user.BaseWorkflowUser" table="EN_USR_T">
    <field-descriptor name="workflowUserId"                 column="PRSN_EN_ID"       jdbc-type="VARCHAR" primarykey="true" autoincrement="true" sequence-name="SEQUENCE_EN_USR" />
    <field-descriptor name="emplId"                         column="PRSN_UNIV_ID"     jdbc-type="VARCHAR" />
    <field-descriptor name="authenticationUserId"           column="PRSN_NTWRK_ID"    jdbc-type="VARCHAR" />
    <field-descriptor name="uuId"                           column="PRSN_UNVL_USR_ID" jdbc-type="VARCHAR" />
    <field-descriptor name="emailAddress"                   column="PRSN_EMAIL_ADDR"  jdbc-type="VARCHAR" />
    <field-descriptor name="displayName"                    column="PRSN_NM"          jdbc-type="VARCHAR"/>
    <field-descriptor name="givenName"                      column="PRSN_GVN_NM"      jdbc-type="VARCHAR"/>
    <field-descriptor name="lastName"                       column="PRSN_LST_NM"      jdbc-type="VARCHAR"/>
  <field-descriptor name="createDate"                     column="USR_CRTE_DT"      jdbc-type="TIMESTAMP"  />
    <field-descriptor name="lastUpdateDate"                 column="USR_LST_UPDT_DT"  jdbc-type="TIMESTAMP" />
    <field-descriptor name="lockVerNbr"                     column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="false"/>
  </class-descriptor>

  <class-descriptor class="edu.iu.uis.eden.workgroup.BaseWorkgroup" table="EN_WRKGRP_T">
    <field-descriptor name="workgroupId" column="WRKGRP_ID" jdbc-type="BIGINT" primarykey="true" />
    <field-descriptor name="versionNumber" column="WRKGRP_VER_NBR" jdbc-type="INTEGER" primarykey="true"/>
  <field-descriptor name="documentId" column="DOC_HDR_ID" jdbc-type="BIGINT"/>
    <field-descriptor name="workgroupName" column="WRKGRP_NM" jdbc-type="VARCHAR" />
    <field-descriptor name="description" column="WRKGRP_DESC" jdbc-type="VARCHAR" />
    <field-descriptor name="activeInd" column="WRKGRP_ACTV_IND" jdbc-type="BIT" />
    <field-descriptor name="workgroupType" column="WRKGRP_TYP_CD" jdbc-type="VARCHAR" />
    <field-descriptor name="currentInd" column="WRKGRP_CUR_IND" jdbc-type="BIT" />
    <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true" />
    <collection-descriptor element-class-ref="edu.iu.uis.eden.workgroup.BaseWorkgroupMember"
        name="workgroupMembers"  auto-retrieve="true" auto-update="true" auto-delete="true">
        <inverse-foreignkey field-ref="workgroupId" refresh="true" />
    <inverse-foreignkey field-ref="workgroupVersionNumber"/>
    </collection-descriptor>
    <collection-descriptor element-class-ref="org.kuali.workflow.workgroup.BaseWorkgroupExtension"
      name="extensions" auto-retrieve="true" auto-update="true" auto-delete="true">
        <inverse-foreignkey field-ref="workgroupId"  />
        <inverse-foreignkey field-ref="workgroupVersionNumber"/>
    </collection-descriptor>
  </class-descriptor>

  <class-descriptor class="edu.iu.uis.eden.workgroup.BaseWorkgroupMember" table="EN_WRKGRP_MBR_T">
    <field-descriptor name="workgroupId" column="WRKGRP_ID" jdbc-type="BIGINT" primarykey="true" />
    <field-descriptor name="workflowId" column="WRKGRP_MBR_PRSN_EN_ID" jdbc-type="VARCHAR"
      primarykey="true" />
    <field-descriptor name="workgroupVersionNumber" column="WRKGRP_VER_NBR" jdbc-type="INTEGER"
      primarykey="true" />
    <field-descriptor name="memberType" column="WRKGRP_MBR_TYP" jdbc-type="VARCHAR"/>
  <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true" />
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="edu.iu.uis.eden.workgroup.BaseWorkgroup" name="workgroup">
      <foreignkey field-ref="workgroupId"/>
    <foreignkey field-ref="workgroupVersionNumber"/>
    </reference-descriptor>
  </class-descriptor>

  <class-descriptor class="org.kuali.workflow.workgroup.BaseWorkgroupExtension" table="EN_WRKGRP_EXT_T">
    <field-descriptor name="workgroupExtensionId" column="WRKGRP_EXT_ID" jdbc-type="BIGINT" primarykey="true" autoincrement="true" sequence-name="SEQ_ROUTE_TEMPLATE"/>
    <field-descriptor name="workgroupId" column="WRKGRP_ID" jdbc-type="BIGINT" access="anonymous"/>
    <field-descriptor name="workgroupVersionNumber" column="WRKGRP_VER_NBR" jdbc-type="INTEGER" access="anonymous" />
    <field-descriptor name="workgroupTypeAttributeId" column="WRKGRP_TYP_ATTRIB_ID" jdbc-type="BIGINT" access="anonymous"/>
    <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true" />
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="edu.iu.uis.eden.workgroup.BaseWorkgroup" name="workgroup">
      <foreignkey field-ref="workgroupId"/>
      <foreignkey field-ref="workgroupVersionNumber"/>
    </reference-descriptor>
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="org.kuali.workflow.workgroup.WorkgroupTypeAttribute" name="workgroupTypeAttribute">
      <foreignkey field-ref="workgroupTypeAttributeId"/>
    </reference-descriptor>
    <collection-descriptor element-class-ref="org.kuali.workflow.workgroup.BaseWorkgroupExtensionData" name="data"
      auto-retrieve="true" auto-update="true" auto-delete="true">
      <inverse-foreignkey field-ref="workgroupExtensionId"/>
    </collection-descriptor>
  </class-descriptor>

  <class-descriptor class="org.kuali.workflow.workgroup.BaseWorkgroupExtensionData" table="EN_WRKGRP_EXT_DTA_T">
    <field-descriptor name="workgroupExtensionDataId" column="WRKGRP_EXT_DTA_ID" jdbc-type="BIGINT" primarykey="true" autoincrement="true" sequence-name="SEQ_ROUTE_TEMPLATE"/>
    <field-descriptor name="workgroupExtensionId" column="WRKGRP_EXT_ID" jdbc-type="BIGINT" access="anonymous"/>
    <field-descriptor name="key" column="EXT_KEY" jdbc-type="VARCHAR"/>
    <field-descriptor name="value" column="EXT_VAL" jdbc-type="VARCHAR"/>
    <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true"/>
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="org.kuali.workflow.workgroup.BaseWorkgroupExtension" name="workgroupExtension">
      <foreignkey field-ref="workgroupExtensionId"/>
    </reference-descriptor>
  </class-descriptor>

  <class-descriptor class="org.kuali.workflow.workgroup.WorkgroupType" table="EN_WRKGRP_TYP_T">
    <field-descriptor name="workgroupTypeId" column="WRKGRP_TYP_ID" jdbc-type="BIGINT" primarykey="true" autoincrement="true" sequence-name="SEQ_ROUTE_TEMPLATE"/>
    <field-descriptor name="name" column="WRKGRP_TYP_NM" jdbc-type="VARCHAR"/>
    <field-descriptor name="label" column="WRKGRP_TYP_LBL" jdbc-type="VARCHAR"/>
    <field-descriptor name="description" column="WRKGRP_TYP_DESC" jdbc-type="VARCHAR"/>
    <field-descriptor name="documentTypeName" column="DOC_TYP_NM" jdbc-type="VARCHAR"/>
    <field-descriptor name="active" column="ACTV_IND" jdbc-type="BIT"/>
    <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true"/>
    <collection-descriptor element-class-ref="org.kuali.workflow.workgroup.WorkgroupTypeAttribute" name="attributes"
      auto-retrieve="true" auto-update="true" auto-delete="true">
      <orderby name="orderIndex" sort="ASC"/>
      <inverse-foreignkey field-ref="workgroupTypeId"/>
    </collection-descriptor>
  </class-descriptor>

  <class-descriptor class="org.kuali.workflow.workgroup.WorkgroupTypeAttribute" table="EN_WRKGRP_TYP_ATTRIB_T">
    <field-descriptor name="workgroupTypeAttributeId" column="WRKGRP_TYP_ATTRIB_ID" jdbc-type="BIGINT" primarykey="true" autoincrement="true" sequence-name="SEQ_ROUTE_TEMPLATE"/>
    <field-descriptor name="workgroupTypeId" column="WRKGRP_TYP_ID" jdbc-type="BIGINT" access="anonymous"/>
    <field-descriptor name="attributeId" column="ATTRIB_ID" jdbc-type="BIGINT" access="anonymous"/>
    <field-descriptor name="active" column="ACTV_IND" jdbc-type="BIT"/>
    <field-descriptor name="orderIndex" column="ORD_INDX" jdbc-type="INTEGER"/>
    <field-descriptor name="lockVerNbr" column="DB_LOCK_VER_NBR" jdbc-type="INTEGER" locking="true"/>
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="org.kuali.workflow.workgroup.WorkgroupType" name="workgroupType">
      <foreignkey field-ref="workgroupTypeId"/>
    </reference-descriptor>
    <reference-descriptor auto-retrieve="true" auto-update="false" auto-delete="false" class-ref="edu.iu.uis.eden.routetemplate.RuleAttribute" name="attribute">
      <foreignkey field-ref="attributeId"/>
    </reference-descriptor>
  </class-descriptor>

</descriptor-repository>