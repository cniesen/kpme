<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0">
    <xsd:element name="root">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="assignments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="assignment" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="tkAssignmentId" type="xsd:long" minOccurs="0"></xsd:element>
                                        <xsd:element name="principalId" type="xsd:string"></xsd:element>
                                        <xsd:element name="jobNumber" type="xsd:long"></xsd:element>
                                        <xsd:element name="effectiveDate" type="xsd:date"></xsd:element>
                                        <xsd:element name="workArea" type="xsd:long"></xsd:element>
                                        <xsd:element name="task" type="xsd:long"></xsd:element>
                                        <xsd:element name="active" type="xsd:boolean"></xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>