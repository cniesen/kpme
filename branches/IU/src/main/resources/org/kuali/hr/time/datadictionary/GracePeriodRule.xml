<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GracePeriodRule" parent="GracePeriodRule-parentBean"/>

  <bean id="GracePeriodRule-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.graceperiod.rule.GracePeriodRule"/>
    <property name="titleAttribute" value="id"/>
    <property name="objectLabel" value="GracePeriodRule"/>
 
    <property name="lookupDefinition">
      <ref bean="GracePeriodRule-lookupDefinition"/>
    </property>
    <property name="attributes">
      <list>
         <ref bean="GracePeriodRule-graceMins"/>
        <ref bean="GracePeriodRule-hourFactor"/>
        <ref bean="GracePeriodRule-effDt"/>
        <ref bean="GracePeriodRule-effStatus"/>
        <ref bean="GracePeriodRule-userEmplId"/>
        <ref bean="GracePeriodRule-timestamp"/>
      </list>
    </property>
  </bean>

  <!-- Attribute Definitions -->
  <bean id="GracePeriodRule-graceMins" parent="GracePeriodRule-graceMins-parentBean"/>

  <bean id="GracePeriodRule-graceMins-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Grace Mins"/>
    <property name="maxLength" value="5"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="5"/>
    </property>
    <property name="name" value="graceMins"/>
    <property name="label" value="Grace Minutes"/>
  </bean>
  

  <bean id="GracePeriodRule-hourFactor" parent="GracePeriodRule-hourFactor-parentBean"/>

  <bean id="GracePeriodRule-hourFactor-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Hr Factor"/>
    <property name="maxLength" value="6"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
    <property name="name" value="hourFactor"/>
    <property name="label" value="Hour Factor"/>
  </bean>


  <bean id="GracePeriodRule-effDt" parent="GracePeriodRule-effDt-parentBean"/>

  <bean id="GracePeriodRule-effDt-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Eff Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="effDt"/>
    <property name="label" value="Effective Date"/>
  </bean>
  
  <bean id="GracePeriodRule-effStatus" parent="GracePeriodRule-effStatus-parentBean"/>

  <bean id="GracePeriodRule-effStatus-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Eff Sequence"/>
    <property name="maxLength" value="1"/>
    <property name="validationPattern">
 		<bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="effStatus"/>
    <property name="label" value="Effective Sequence"/>
  </bean>
 
 
  <bean id="GracePeriodRule-userEmplId" parent="GracePeriodRule-userEmplId-parentBean"/>

  <bean id="GracePeriodRule-userEmplId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Empl Id"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="AnyCharacterValidationPattern">
       		<property name="allowWhitespace" value="false"/>
       	</bean>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"  />
    </property>
    <property name="name" value="userEmplId"/>
    <property name="label" value="User University Id"/>
  </bean>

   <bean id="GracePeriodRule-timestamp" parent="GracePeriodRule-timestamp-parentBean"/>

  <bean id="GracePeriodRule-timestamp-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Time Stamp"/>
    <property name="maxLength" value="20"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"  />
    </property>
    <property name="name" value="timestamp"/>
    <property name="label" value="Time Stamp"/>
  </bean>
  
  
  
  
  <!--  Inquiry Definitions  -->
	
  <bean id="GracePeriodRule-lookupDefinition" parent="GracePeriodRule-lookupDefinition-parentBean"/>

  <bean id="GracePeriodRule-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupFields">
      <list>
          <bean parent="FieldDefinition" p:attributeName="graceMins"/>
        <bean parent="FieldDefinition" p:attributeName="effDt"/>
        <bean parent="FieldDefinition" p:attributeName="effStatus"/>
        <bean parent="FieldDefinition" p:attributeName="userEmplId"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
         <bean parent="FieldDefinition" p:attributeName="graceMins"/>
        <bean parent="FieldDefinition" p:attributeName="hourFactor"/>
        <bean parent="FieldDefinition" p:attributeName="effDt"/>
        <bean parent="FieldDefinition" p:attributeName="effStatus"/>
        <bean parent="FieldDefinition" p:attributeName="userEmplId"/>
        <bean parent="FieldDefinition" p:attributeName="timestamp"/>
      </list>
    </property>
    <property name="title" value="Grace Period Rule Lookup"/>
    
  </bean>

</beans>
